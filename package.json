{
    "name": "gitflow",
    "displayName": "gitflow",
    "description": "Gitflow integration and support in Visual Studio Code",
    "version": "0.0.1",
    "publisher": "vector-of-bool",
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:gitflow.featureStart",
        "onCommand:gitflow.featureFinish",
        "onCommand:gitflow.initialize"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "gitflow.initialize",
            "title": "Gitflow: Initialize repository for gitflow"
        }, {
            "command": "gitflow.featureStart",
            "title": "Gitflow: feature: start"
        }, {
            "command": "gitflow.featureFinish",
            "title": "Gitflow: feature: finish"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}